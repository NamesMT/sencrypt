{
  "name": "@namesmt/sencrypt",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@9.15.1",
  "description": "",
  "author": "NamesMT <dangquoctrung123@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/namesmt",
  "homepage": "https://github.com/namesmt/sencrypt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namesmt/sencrypt.git"
  },
  "bugs": "https://github.com/namesmt/sencrypt/issues",
  "keywords": [
    "encrypt",
    "salt",
    "secret"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "start": "NODE_ENV=dev tsx src/index.ts",
    "watch": "NODE_ENV=dev tsx watch src/index.ts",
    "stub": "unbuild --stub",
    "dev": "pnpm run watch",
    "play": "pnpm run stub && pnpm run --filter playground dev",
    "play:useBuild": "pnpm run build && pnpm run --filter playground dev",
    "lint": "eslint .",
    "test": "vitest",
    "test:types": "tsc --noEmit --skipLibCheck",
    "check": "pnpm lint && pnpm test:types && vitest run --coverage",
    "build": "unbuild",
    "release": "pnpm dlx changelogen@latest --release --push --publish",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@namesmt/shash": ">=0.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@namesmt/aes-gcm": "^0.1.3",
    "@namesmt/utils": "^0.5.9",
    "@types/node": "^20.17.10",
    "@vitest/coverage-v8": "^2.1.8",
    "consola": "^3.3.0",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "simple-git-hooks": "^2.11.1",
    "std-env": "^3.8.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "hasown": "npm:@nolyfill/hasown@^1.0.29",
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
